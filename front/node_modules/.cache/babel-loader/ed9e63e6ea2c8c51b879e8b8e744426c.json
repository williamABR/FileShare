{"ast":null,"code":"export default {\n  name: \"HelloWorld\",\n  props: {\n    msg: String\n  },\n  data: function setCookie(cname, cvalue, exdays) {\n    const d = new Date();\n    d.setTime(d.getTime() + exdays * 24 * 60 * 60 * 1000);\n    let expires = \"expires=\" + d.toUTCString();\n    document.cookie = cname + \"=\" + cvalue + \";\" + expires + \";path=/\";\n  },\n  data: function checkCookie(cname) {\n    let user = getCookie(cname);\n\n    if (user != \"\") {\n      alert(\"Welcome again \" + user);\n      return true;\n    } else {\n      return false;\n      /*user = prompt(\"Please enter your name:\",\"\");\n       if (user != \"\" && user != null) {\n         setCookie(\"username\", user, 30);\n       }*/\n    }\n  } //setCookie(\"prueba\",\"123456\",\"1\");\n\n};\n\nfunction getCookie(cname) {\n  let name = cname + \"=\";\n  let decodedCookie = decodeURIComponent(document.cookie);\n  let ca = decodedCookie.split(\";\");\n\n  for (let i = 0; i < ca.length; i++) {\n    let c = ca[i];\n\n    while (c.charAt(0) == \" \") {\n      c = c.substring(1);\n    }\n\n    if (c.indexOf(name) == 0) {\n      return c.substring(name.length, c.length);\n    }\n  }\n\n  return \"\";\n}","map":{"version":3,"mappings":"AAmFA,eAAe;EACbA,IAAI,EAAE,YADO;EAEbC,KAAK,EAAE;IACLC,GAAG,EAAEC;EADA,CAFM;EAKbC,IAAI,EAAE,SAASC,SAAT,CAAmBC,KAAnB,EAA0BC,MAA1B,EAAkCC,MAAlC,EAA0C;IAC9C,MAAMC,IAAI,IAAIC,IAAJ,EAAV;IACAD,CAAC,CAACE,OAAF,CAAUF,CAAC,CAACG,OAAF,KAAcJ,MAAK,GAAI,EAAT,GAAc,EAAd,GAAmB,EAAnB,GAAwB,IAAhD;IACA,IAAIK,OAAM,GAAI,aAAaJ,CAAC,CAACK,WAAF,EAA3B;IACAC,QAAQ,CAACC,MAAT,GAAkBV,KAAI,GAAI,GAAR,GAAcC,MAAd,GAAuB,GAAvB,GAA6BM,OAA7B,GAAuC,SAAzD;EACD,CAVY;EAWbT,IAAI,EAAE,SAASa,WAAT,CAAqBX,KAArB,EAA4B;IAClC,IAAIY,IAAG,GAAIC,SAAS,CAACb,KAAD,CAApB;;IACA,IAAIY,IAAG,IAAK,EAAZ,EAAgB;MACdE,KAAK,CAAC,mBAAmBF,IAApB,CAAL;MACA,OAAO,IAAP;IACF,CAHA,MAGO;MACL,OAAO,KAAP;MACA;;;;IAIF;EACD,CAvBc,CAwBb;;AAxBa,CAAf;;AA2BA,SAASC,SAAT,CAAmBb,KAAnB,EAA0B;EACxB,IAAIN,IAAG,GAAIM,KAAI,GAAI,GAAnB;EACA,IAAIe,aAAY